package entity;

public class Mochila extends Produto{
	private String marca;

	public Mochila(String nome, String cnpj, String descricao, String codigo, double valorUnitario, int qtdeEstoque,
			String marca) {
		super(nome, cnpj, descricao, codigo, valorUnitario, qtdeEstoque);
		this.marca = marca;
	}

	public String getMarca() {
		return marca;
	}

	public void setMarca(String marca) {
		this.marca = marca;
	}	
}
