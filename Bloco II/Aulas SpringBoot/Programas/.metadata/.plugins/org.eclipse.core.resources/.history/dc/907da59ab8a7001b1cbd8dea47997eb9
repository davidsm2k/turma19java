package com.turma19.cadastro.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.turma19.cadastro.model.Pessoa;

@Repository
public interface CadastroRepository extends JpaRepository<Pessoa, Long>{
	public List<Pessoa> findAllByNomeContainingIgnoreCase (String nome);
}
